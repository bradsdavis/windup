<?xml version="1.0" encoding="UTF-8"?>
<configuration xmlns:c="http://windup.jboss.org/v1/configuration" xmlns:j="http://windup.jboss.org/v1/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<vendors>
		<vendor name="Apache">
			 <package-structure pattern="org.apache.*"/>
			 <namespace uri=""/>
		</vendor>
		<vendor name="Etc">
			<package-structure pattern="org.etc.*"/>
		</vendor>
	</vendors>
	
	
	<extensions>
		<extension type="java-source">
			<value>.java</value>
		</extension>
		<extension type="java-class">
			<extension>.class</extension>
		</extension>
		<extension type="xml">
			<extension>.xml</extension>
			<extension>.tld</extension>
		</extension>
		<extension type="zip">
			<extension>.ear</extension>
			<extension>.war</extension>
		</extension>
	</extensions>

</configuration>